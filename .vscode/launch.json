  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:5000",
      "webRoot": "${workspaceRoot}/wwwroot"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Node.js",
      "program": "${workspaceRoot}/wwwroot/out/bundle.js",
      "args": [],
      "runtimeArgs": [
        "--expose-wasm"
      ],
      "outFiles": [
        "${workspaceRoot}/wwwroot/out/**/*.js"
      ]
    }
  ]
}